CREATE TABLE dates(
  time TIME NOT NULL,
  date DATE NOT NULL,
  datetime DATETIME NOT NULL,
  timestamp TIMESTAMP NOT NULL,
  year YEAR NOT NULL
);

insertDate {
INSERT INTO dates VALUES (?, ?, ?, ?, ?);

SELECT * FROM dates LIMIT 1;
}

minDates:
SELECT min(time), min(date), min(datetime), min(timestamp), min(year) FROM dates;

maxDates:
SELECT max(time), max(date), max(datetime), max(timestamp), max(year) FROM dates;

CREATE TABLE dates_lowercase(
  time time NOT NULL,
  date date NOT NULL,
  datetime datetime NOT NULL,
  timestamp timestamp NOT NULL,
  year year NOT NULL
);
